{
  "name": "reubenson",
  "version": "1.0.1",
  "description": "Homepage of software developer and musician, Reuben Son",
  "author": "reuben son",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "split": "git subtree split --prefix dist -b gh-pages",
    "pre-deploy": "npm run generate && git add . && git commit -m 'pre-deploy commit' && npm version patch",
    "post-deploy": "git branch -D gh-pages",
    "deploy": "npm run pre-deploy && git push origin dev && npm run split --prefix dist -b gh-pages && git push -f origin gh-pages:master && npm run post-deploy"
  },
  "dependencies": {
    "nuxt": "^2.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4"
  },
  "devDependencies": { 
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.4.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.0"
  }
}
